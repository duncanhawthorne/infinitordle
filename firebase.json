{
  "hosting": {
    "public": "build/web",  
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-central1"
    }
  }, 
  "headers": [{
      "source": "**/*.@(jpg|jpeg|gif|png|html)",
      "headers": [ 
	        {
        "key": "Cross-Origin-Embedder-Policy",
        "value": "credentialless"
      },
      {
        "key": "Cross-Origin-Opener-Policy",
        "value": "same-origin"
      }
	  ]
    }, 
	{
      "source": "**",
      "headers": [ 
	        {
        "key": "Cross-Origin-Embedder-Policy",
        "value": "credentialless"
      },
      {
        "key": "Cross-Origin-Opener-Policy",
        "value": "same-origin"
      }
	  ]
    } ]
}